views:
  - type: table
    name: Table
    filters:
      and:
        - file.ext == "md"
        - file.tags.contains("lecture")
        - '!file.name.endsWith("Template")'
        - class == "ECON 1BB3"
    order:
      - file.name
      - CreatedAt
    sort:
      - property: file.ctime
        direction: ASC
