views:
  - type: table
    name: Table
    filters:
      and:
        - file.ext == "md"
        - file.tags.contains("lecture")
    sort:
      - property: file.ctime
        direction: ASC
  - type: table
    name: View
    filters:
      and:
        - file.ext == "md"
        - file.tags.contains("lecture")
    sort:
      - property: file.ctime
        direction: ASC
